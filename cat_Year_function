//cat year conversion age


var catYear = function(myAge,myName){
  var earlyYears = 2;
  earlyYears *= 25;
  var laterYears = myAge - 2;
  laterYears *= 4;
  var myAgeInCatYears = earlyYears + laterYears;
  
  console.log("My name is " + myName + ". I am " 
  + myAge + " years old in human age which is "
  + myAgeInCatYears + " \n years old in cat years.");
}
